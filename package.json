{
  "name": "devarmory",
  "displayName": "DevArmory",
  "description": "Build Faster With Your Dev Switch Blade.",
  "version": "0.0.17",
  "publisher": "CharithBhat",
  "license": "GPLv3",
  "icon": "media/logo-helmet.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/CharithBhat/DevArmory"
  },
  "keywords": [
    "Dev Armory",
    "dev armory",
    "devarmory",
    "devarmor",
    "armor",
    "dev armor",
    "Dev Armor",
    "Dev armor"
  ],
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other",
    "Formatters",
    "Linters",
    "Snippets"
  ],
  "activationEvents": [
    "onView:devarmory-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "devarmory-sidebar-view",
          "title": "DevArmory",
          "icon": "media/vs-code-icon.svg"
        }
      ]
    },
    "views": {
      "devarmory-sidebar-view": [
        {
          "type": "webview",
          "id": "devarmory-sidebar",
          "name": "DevArmory",
          "icon": "media/vs-code-icon.svg",
          "contextualTitle": "DevArmory"
        }
      ]
    },
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c && webpack --mode production --devtool inline-source-map",
    "watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
    "package": "webpack --mode production --devtool inline-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-typescript": "^8.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/uuid": "^8.3.4",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "concurrently": "^7.4.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sql-formatter": "^11.0.2",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "util": "^0.12.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "blueimp-md5": "^2.19.0"
  }
}
